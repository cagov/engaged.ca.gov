{% set active_page = 'stateemployees' %}
{% extends "layout.njk" %}
{% block content %}

<link rel="stylesheet" href="/css/state-employees.css" />
<link rel="stylesheet" href="/css/state-employees-report.css" />


<section id="e3-report-hero" class="hero">
  <div class="container hero-container">
    <div class="row">
      <div class="col-lg-8">
        Hero content
        <p>
            <a data-appearance="button" data-theme="orange" href="#findings-top-options" class="m-t">Button text</a>
        </p>

      </div>
    </div>
    <img class="findings-adornment" src="/public/images/findings-adornment.png" aria-hidden="true" alt="" />
  </div>
</section>

<section id="e3-report-stats">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
            <div class="stats-intro">
             <h2>Title</h2>
            </div>
      </div>
    </div>
  </div>
</section>

<section id="e3-report-sharings">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
            <div class="stats-intro">
             <h2>What staff shared</h2>
            </div>
      </div>
    </div>
  </div>
</section>

<section id="e3-themes">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
            <div class="stats-intro">
             <h2>Themes</h2>
            </div>
      </div>
    </div>
  </div>
</section>

<section id="e3-comment-explorer">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
            <div class="stats-intro">
             <h2>Comment explorer</h2>
            </div>
      </div>
    </div>
  </div>
</section>

<section id="e3-response">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
            <div class="stats-intro">
             <h2>Response</h2>
            </div>
      </div>
    </div>
  </div>
</section>



{% endblock %}